<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" xmlns:nsExToolsShared="dummy">
<ribbon startFromScratch="false">
<tabs>
<tab idQ="nsExToolsShared:ExToolsTab" label="拡張" insertAfterMso="TabView" keytip="B">

	<!--- **********[CellOperation]********** -->
	<group idQ="nsExToolsShared:Group3" label="値の操作" insertBeforeQ="nsExToolsShared:Group4">
		<button id="Copy2CBShapeText" label="シェイプのテキストをコピー" keytip="L" size="normal" imageMso="TextDialog" onAction="ribbonCallback_Copy2CB" />
	</group>

	<!--- **********[Boarders/Charts]********** -->
	<group idQ="nsExToolsShared:Group4" label="オブジェクト" insertBeforeQ="nsExToolsShared:Group6">
			<button id="AdjShapeBlockArrow" label="ブロック矢印の先端を揃える" image="homebase16" onAction="ribbonCallback_AdjustShape" />
			<button id="AdjShapeRoundRect" label="角丸四角形の丸みを揃える" image="roundRect16" onAction="ribbonCallback_AdjustShape" />
			<button id="AdjShapeElbowConn" label="鍵コネクタの曲がり位置を揃える" imageMso="PivotDiagramAlignCenter" onAction="ribbonCallback_AdjustShape" />
			<button id="AdjShapeAngle" label="円の角度を設定する" image="arch16" onAction="ribbonCallback" />
			<button id="AdjShapeLine" label="線の角度を揃える" imageMso="LineTool" onAction="ribbonCallback_AdjustShape" />
			<splitButton id="slbSample" size="normal">
				<button id="AdjShapeOrderTile" label="タイル状に揃える" image="tileLayout16" onAction="ribbonCallback_AdjustShape" />
				<menu id="Group4Menu1">
					<button id="AdjShapeOrderTile_1" label="間隔：1pt" onAction="ribbonCallback_AdjustShape" />
					<button id="AdjShapeOrderTile_2" label="間隔：2pt" onAction="ribbonCallback_AdjustShape" />
					<button id="AdjShapeOrderTile_3" label="間隔：3pt" onAction="ribbonCallback_AdjustShape" />
					<button id="AdjShapeOrderTile_4" label="間隔：4pt" onAction="ribbonCallback_AdjustShape" />
				</menu>
			</splitButton>
			<button id="AdjShapeUngroup" label="グループを全て解除" imageMso="ObjectsUngroup" onAction="ribbonCallback_AdjustShape" />
	</group>

	<!--- **********[Others]********** -->
	<group idQ="nsExToolsShared:Group6" label="その他" insertAfterQ="nsExToolsShared:Group4">
		<button id="Version" label="バージョン情報"  size="normal" imageMso="RefreshStatus" onAction="ribbonCallback" />
	</group>

</tab>
</tabs>
</ribbon>
</customUI>
